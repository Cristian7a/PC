{
  "name": "@nx-workspace/source",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=24.7.0",
    "npm": ">=11.0.0"
  },
  "scripts": {
    "nx": "nx",
    "client:build": "nx build client",
    "client:start": "nx serve client",
    "client:watch": "nx build client --watch --configuration development",
    "prepare": "rm -rf ./.husky && cd ../ && husky ./nx-workspace/.husky && cd nx-workspace && echo \"cd ./nx-workspace && npm run precommit\" > .husky/pre-commit",
    "precommit": "lint-staged",
    "server:build": "nx build server",
    "server:build:dev": "nx build server -c development",
    "server:start": "node dist/apps/server/src/server.js",
    "server:dev": "nx serve server -c development",
    "server:migrate:internal": "node-pg-migrate --config-file config/pgmigrate.json",
    "server:migrate": "npm run server:migrate:internal up",
    "server:migrate:down": "npm run server:migrate:internal down",
    "server:migrate:reset": "npm run server:migrate:internal down 0",
    "server:migrate:create": "npm run server:migrate:internal create",
    "server:seed:dev": "node apps/server/src/seeders/dev.ts",
    "server:force-reset-db:dev": "npm run server:migrate:reset && npm run server:build && npm run server:migrate && npm run server:seed:dev"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.3.1",
    "@angular/compiler": "20.3.1",
    "@angular/core": "20.3.1",
    "@angular/forms": "20.3.1",
    "@angular/platform-browser": "20.3.1",
    "@angular/platform-browser-dynamic": "20.3.1",
    "@angular/router": "20.3.1",
    "@ngrx/component-store": "20.0.1",
    "@ngrx/effects": "20.0.1",
    "@ngrx/entity": "20.0.1",
    "@ngrx/router-store": "20.0.1",
    "@ngrx/store": "20.0.1",
    "@ngx-translate/core": "17.0.0",
    "@ngx-translate/http-loader": "17.0.0",
    "@primeng/themes": "20.1.2",
    "bcrypt": "6.0.0",
    "cors": "2.8.5",
    "dotenv": "17.2.2",
    "express": "5.1.0",
    "express-jwt": "8.5.1",
    "jsonwebtoken": "9.0.2",
    "pg": "8.16.3",
    "primeicons": "7.0.0",
    "primeng": "20.1.2",
    "rxjs": "7.8.2",
    "uuid": "13.0.0",
    "zod": "4.1.11",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "1.21.1",
    "@analogjs/vitest-angular": "1.21.1",
    "@angular-devkit/build-angular": "20.3.2",
    "@angular-devkit/core": "20.3.2",
    "@angular-devkit/schematics": "20.3.2",
    "@angular/cli": "20.3.2",
    "@angular/compiler-cli": "20.3.1",
    "@angular/language-service": "20.3.1",
    "@eslint/js": "9.36.0",
    "@ngrx/schematics": "20.0.1",
    "@ngrx/store-devtools": "20.0.1",
    "@nx/angular": "21.5.3",
    "@nx/esbuild": "21.5.3",
    "@nx/eslint": "21.5.3",
    "@nx/eslint-plugin": "21.5.3",
    "@nx/js": "21.5.3",
    "@nx/node": "21.5.3",
    "@nx/vite": "21.5.3",
    "@nx/web": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@schematics/angular": "20.3.2",
    "@swc-node/register": "1.11.1",
    "@swc/core": "1.13.5",
    "@swc/helpers": "0.5.17",
    "@tailwindcss/postcss": "4.1.13",
    "@types/bcrypt": "6.0.0",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/node": "24.5.2",
    "@types/pg": "8.15.5",
    "@typescript-eslint/utils": "8.44.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "angular-eslint": "20.3.0",
    "config": "4.1.1",
    "esbuild": "0.25.10",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "jiti": "2.6.0",
    "jsdom": "27.0.0",
    "lint-staged": "16.2.0",
    "node-pg-migrate": "8.0.3",
    "nx": "21.5.3",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-eslint": "16.4.2",
    "tailwindcss": "4.1.13",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vite": "7.1.7",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "nx affected --target=lint"
    ],
    "*.{html,css,scss}": [
      "prettier --write"
    ]
  }
}
